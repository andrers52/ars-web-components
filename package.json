{
    "name": "ars-web-components",
    "version": "0.7.3",
    "description": "A collection of reusable web components built with vanilla JavaScript and ES modules",
    "repository": "https://github.com/andrers52/ars-web-components.git",
    "main": "index.js",
    "scripts": {
        "start": "node server.js",
        "stop": "lsof -ti:8080 | xargs kill -9",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
        "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
    },
    "devDependencies": {
        "@babel/core": "^7.29.0",
        "@babel/preset-env": "^7.29.0",
        "@jest/globals": "^30.2.0",
        "babel-jest": "^30.2.0",
        "express": "5.1.0",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0"
    },
    "keywords": [
        "web-components",
        "custom-elements",
        "vanilla-javascript",
        "es-modules",
        "ui-components",
        "frontend",
        "reusable-components"
    ],
    "dependencies": {
        "arslib": "0.7.0"
    },
    "author": "Andre Rodrigues da Silva",
    "license": "MIT"
